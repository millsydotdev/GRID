{
  "all": true,
  "include": [
    "src/vs/workbench/contrib/grid/**/*.ts",
    "src/vs/workbench/contrib/grid/**/*.js"
  ],
  "exclude": [
    "**/*.test.ts",
    "**/*.test.js",
    "**/test/**",
    "**/node_modules/**",
    "**/out/**",
    "src/vs/workbench/contrib/grid/browser/react/**"
  ],
  "reporter": [
    "text",
    "text-summary",
    "html",
    "lcov",
    "json",
    "cobertura"
  ],
  "report-dir": "./coverage",
  "temp-dir": "./.nyc_output",
  "extension": [
    ".ts",
    ".js"
  ],
  "require": [
    "ts-node/register"
  ],
  "sourceMap": true,
  "instrument": true,
  "cache": true,
  "check-coverage": true,
  "lines": 20,
  "statements": 20,
  "functions": 15,
  "branches": 15,
  "per-file": false,
  "skip-full": false,
  "watermarks": {
    "lines": [50, 80],
    "functions": [50, 80],
    "branches": [50, 80],
    "statements": [50, 80]
  }
}
